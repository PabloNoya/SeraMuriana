      "settings": {
        "foreground": "#eacda2",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#4bb1db",
        "fontStyle": ""
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#5acece",
        "fontStyle": "italic"
      }
    },
    {
      "name": "CSS attribute",
