      "settings": {
        "foreground": "#7fd786"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#7fd786",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "foreground": "#4bb1db",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Entity name",
      "scope": [
        "entity.name.function",
        //python
        "meta.function-call.python",
        "support.function.builtin.python"
