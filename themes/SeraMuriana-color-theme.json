			"settings": {
				"foreground": "#ecd3ae",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#D8DEE9DD"
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"foreground": "#D8DEE9DD"
			}
		},
