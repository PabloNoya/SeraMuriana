      "settings": {
        "foreground": "#f15175",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Annotation Punctuation",
      "scope": "punctuation.definition.annotation",
      "settings": {
        "foreground": "#f15175",
        "fontStyle": "bold"
