{
  "name": "SērāMūriana",
  "tokenColors": [
    {
      "name": "Source",
      "scope": "source",
      "settings": {
        "foreground": "#f2f0fd"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#c2c0cc"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment.block.documentation"],
      "settings": {
        "foreground": "#c0c7d7"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#d7b8ea",
        "fontStyle": ""
      }
    },
    {
      "name": "Template string var",
      "scope": "meta.template.expression.js",
      "settings": {
        "foreground": "#f2f0fd",
        "fontStyle": ""
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.definition",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "storage.type.string"
      ],
      "settings": {
        "foreground": "#fabdd4",
        "fontStyle": ""
      }
    },
    {
      "name": "Punctuation variable",
      "scope": "punctuation.definition.variable",
      "settings": {
        "foreground": "#f5eb94",
        "fontStyle": ""
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#61d6c6"
      }
    },
    {
      "name": "Brackets",
      "scope": [
        "punctuation.definition.begin.bracket",
        "punctuation.definition.end.bracket",
        "punctuation.section.arguments.begin.bracket",
        "punctuation.section.arguments.end.bracket",
        "punctuation.definition.parameters"
      ],
      "settings": {
        "foreground": "#f2f0fd",
        "fontStyle": ""
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric", "storage.type.imaginary.number"],
      "settings": {
        "foreground": "#fbb66f",
        "fontStyle": ""
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#efa3ce"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other.caps.python"],
      "settings": {
        "foreground": "#efa3ce",
        "fontStyle": ""
      }
    },
    {
      "name": "Member Variable",
      "scope": "variable.member",
      "settings": {
        "foreground": "#f56684"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.operator.word",
        "keyword.operator.new.js",
        "keyword.control",
        "keyword.operator.expression.in.js",
        "keyword.operator.expression.of.js"
      ],
      "settings": {
        "foreground": "#55b8e2",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword SQL",
      "scope": ["keyword.other.sql", "keyword.other.postgres"],
      "settings": {
        "foreground": "#55b8e2",
        "fontStyle": ""
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#f7df6e",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Logical Operator python",
      "scope": "keyword.operator.logical.python",
      "settings": {
        "foreground": "#fabdd4",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Javascript instanceof",
      "scope": "keyword.operator.expression.instanceof",
      "settings": {
        "foreground": "#fabdd4",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation.terminator", "punctuation.separator"],
      "settings": {
        "foreground": "#f5eb94",
        "fontStyle": "bold"
      }
    },
    {
      "name": "HTML CSS punctuation",
      "scope": ["punctuation.separator.key-value", "punctuation.definition.entity.css"],
      "settings": {
        "foreground": "#f7df6e",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation.section"],
      "settings": {
        "foreground": "#f5eb94",
        "fontStyle": "bold"
      }
    },
    {
      "name": "php block",
      "scope": ["punctuation.section.embedded.begin.php", "punctuation.section.embedded.end.php"],
      "settings": {
        "foreground": "#61d6c6",
        "fontStyle": ""
      }
    },
    {
      "name": "Accessor",
      "scope": [
        "punctuation.accessor",
        "punctuation.separator.dot-access",
        "punctuation.separator.pointer-access",
