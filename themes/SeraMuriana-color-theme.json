      "settings": {
        "foreground": "#7cd5a1"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#7cd5a1",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "foreground": "#55b8e2",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Entity name",
