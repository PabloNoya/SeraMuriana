			"settings": {
				"foreground": "#f8b858"
			}
		},
		{
			"name": "markup code",
			"scope": "markup.raw",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"name": "markup code",
			"scope": "markup.raw.inline",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"name": "markup punctuation",
