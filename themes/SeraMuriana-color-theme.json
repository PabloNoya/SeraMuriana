{
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#B0B0B7"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment.block.documentation"],
      "settings": {
        "foreground": "#c0c7d7"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#cea9e3",
        "fontStyle": ""
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation.definition",
      "settings": {
        "foreground": "#ff9fc3",
        "fontStyle": ""
      }
    },
    {
      "name": "Punctuation variable",
      "scope": "punctuation.definition.variable",
      "settings": {
        "foreground": "#f3ee80",
        "fontStyle": ""
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#5acece"
      }
    },
    {
      "name": "Brackets",
      "scope": [
        "punctuation.definition.begin.bracket",
        "punctuation.definition.end.bracket",
        "punctuation.section.arguments.begin.bracket",
        "punctuation.section.arguments.end.bracket",
        "punctuation.definition.parameters",
        //python
        "punctuation.definition.list.begin.python",
        "punctuation.definition.list.end.python",
        "punctuation.definition.arguments.begin.python",
        "punctuation.definition.arguments.end.python",
        //php
        "punctuation.section.array.begin.php",
        "punctuation.section.array.end.php"
      ],
      "settings": {
        "foreground": "#F2F0FD",
        "fontStyle": ""
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric", "storage.type.imaginary.number"],
      "settings": {
        "foreground": "#f8c270",
        "fontStyle": ""
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ec88bb"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character"],
      "settings": {
        "foreground": "#ec88bb",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Other constant",
      "scope": "constant.other",
      "settings": {
        "foreground": "#ff9fc3"
      }
    },
    {
      "name": "Member Variable",
      "scope": "variable.member",
      "settings": {
        "foreground": "#f15175"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.operator.word",
        "keyword.operator.new.js",
        "keyword.control"
      ],
      "settings": {
        "foreground": "#4bb1db",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword SQL",
      "scope": ["keyword.other.sql", "keyword.other.postgres"],
      "settings": {
        "foreground": "#4bb1db",
        "fontStyle": ""
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#EED33A",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Logical Operator python",
      "scope": "keyword.operator.logical.python",
      "settings": {
        "foreground": "#ff9fc3",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation.terminator", "punctuation.separator"],
      "settings": {
        "foreground": "#f3ee80",
        "fontStyle": "bold"
      }
    },
    {
      "name": "HTML CSS punctuation",
      "scope": [
        "punctuation.separator.key-value",
        "punctuation.definition.entity.css"
