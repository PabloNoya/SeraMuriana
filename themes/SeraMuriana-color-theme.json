{
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#B0B8D0"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#C8A0E0",
				"fontStyle": ""
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation.definition",
			"settings": {
				"foreground": "#ff9fc3",
				"fontStyle": ""
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation.definition.tag",
			"settings": {
				"foreground": "#40C0C0"
			}
		},
		{
			"name": "Brackets",
			"scope": [
				"punctuation.definition.begin.bracket",
				"punctuation.definition.end.bracket",
				"punctuation.section.arguments.begin.bracket",
				"punctuation.section.arguments.end.bracket",
				"punctuation.definition.parameters",
				//python
				"punctuation.definition.list.begin.python",
				"punctuation.definition.list.end.python",
				"punctuation.definition.arguments.begin.python",
				"punctuation.definition.arguments.end.python",
				//php
				"punctuation.section.array.begin.php",
				"punctuation.section.array.end.php"
			],
			"settings": {
				"foreground": "#F2F0FD",
				"fontStyle": ""
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#F8B858",
				"fontStyle": "",
			}
		},
		{
			"name": "Built-in constant",
			"scope": "constant.language",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#e768a9"
			}
		},
		{
			"name": "User-defined constant",
			"scope": [
				"constant.character"
			],
			"settings": {
				"foreground": "#e768a9",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Other constant",
			"scope": "constant.other",
			"settings": {
				"foreground": "#ff9fc3"
			}
		},
		{
			"name": "Member Variable",
			"scope": "variable.member",
			"settings": {
				"foreground": "#ef3f66"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword - keyword.operator",
				"keyword.operator.word",
				"keyword.operator.new.js",
				"keyword.control"
			],
			"settings": {
				"foreground": "#40A8D8",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#EED33A",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Logical Operator python",
			"scope": "keyword.operator.logical.python",
			"settings": {
				"foreground": "#ff9fc3",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation.terminator",
				"punctuation.separator",
			],
			"settings": {
				"foreground": "#F7F39E",
				"fontStyle": "bold"
			}
		},
		{
			"name": "HTML CSS punctuation",
			"scope": [
				"punctuation.separator.key-value",
				"punctuation.definition.entity.css"
			],
			"settings": {
				"foreground": "#F7F39E",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation.section",
			],
			"settings": {
				"foreground": "#F7F39E",
				"fontStyle": "bold"
			}
		},
		{
			"name": "php block",
			"scope": [
				"punctuation.section.embedded.begin.php",
				"punctuation.section.embedded.end.php"
			],
			"settings": {
				"foreground": "#40C0C0",
				"fontStyle": ""
			}
		},
		{
			"name": "Accessor",
			"scope": [
				"punctuation.accessor",
				"punctuation.separator.dot-access",
				"punctuation.separator.pointer-access",
				//python
				"punctuation.separator.period.python",
			],
			"settings": {
				"foreground": "#ef3f66",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Annotation Punctuation",
			"scope": "punctuation.definition.annotation",
			"settings": {
				"foreground": "#ef3f66",
				"fontStyle": "bold"
			}
		},
		{
			"name": "JavaScript Dollar",
			"scope": [
				"variable.other.dollar.only.js",
				"variable.other.object.dollar.only.js",
				"variable.type.dollar.only.js",
				"support.class.dollar.only.js"
			],
			"settings": {
				"foreground": "#68D070"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"foreground": "#eed33a",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Storage type",
			"scope": "storage.type",
			"settings": {
				"foreground": "#40A8D8",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Entity name",
			"scope": [
				"entity.name.function",
				//python
				"meta.function-call.python",
				"support.function.builtin.python"
			],
			"settings": {
				"foreground": "#40C0C0",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Entity name",
			"scope": "entity.name - (entity.name.section | entity.name.tag | entity.name.label)",
			"settings": {
				"foreground": "#40C0C0",
				"fontStyle": ""
			}
		},
		{
			"name": "Inherited class",
			"scope": [
				"entity.other.inherited-class"
			],
			"settings": {
				"fontStyle": "italic underline",
				"foreground": "#68D070"
			}
		},
		{
			"name": "Function argument",
			"scope": [
				"variable.parameter",
				"meta.function-call.arguments"
			],
			"settings": {
				"foreground": "#F2F0FD",
				"fontStyle": ""
			}
		},
		{
			"name": "Language variable",
			"scope": "variable.language",
			"settings": {
				"foreground": "#e768a9",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Tag name",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#40A8D8",
				"fontStyle": ""
			}
		},
		{
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#40C0C0",
				"fontStyle": "italic"
			}
		},
		{
			"name": "CSS attribute",
			"scope": "entity.other.attribute-name.pseudo-class.css",
			"settings": {
				"foreground": "#C8A0E0",
				"fontStyle": ""
			}
		},
		{
			"name": "Function call",
			"scope": [
				"variable.function",
				"variable.annotation"
			],
			"settings": {
				"foreground": "#68D070",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Library function",
			"scope": [
				"support.function",
				"support.macro",
				"keyword.operator.sizeof",
				//python
				"support.type.python"
			],
			"settings": {
				"foreground": "#eed33a",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Library constant",
			"scope": "support.constant",
			"settings": {
				"foreground": "#C8A0E0",
				"fontStyle": "italic"
				//P's hair
				//"foreground": "#58A088",
			}
		},
		{
			"name": "Library class/type",
			"scope": [
				"support.type",
				"support.class"
