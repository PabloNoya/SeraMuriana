      "settings": {
        "foreground": "#eed33a",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation.section"],
      "settings": {
        "foreground": "#f3ee80",
        "fontStyle": "bold"
      }
    },
    {
      "name": "php block",
      "scope": [
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.end.php"
      ],
      "settings": {
        "foreground": "#5acece",
        "fontStyle": ""
      }
    },
    {
      "name": "Accessor",
      "scope": [
        "punctuation.accessor",
        "punctuation.separator.dot-access",
        "punctuation.separator.pointer-access",
